void setup() {
  uint16_t i;

  pinMode(SER, OUTPUT);
  pinMode(RCLK, OUTPUT);
  pinMode(SRCLK, OUTPUT);

  while(1){
    for(i = 0; i < SAMPLE; i++){
      digitalWrite(RCLK, LOW);
      shiftOut(SER, SRCLK, MSBFIRST, puttrns[i]);
      digitalWrite(RCLK, HIGH);
      delayMicroseconds(1500);
    }
  }
}

